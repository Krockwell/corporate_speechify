{
  "name": "Corporate Speechify",
  "description": "Eliminate corporate double speak",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Kevin Rockwell",

  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
